{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "title": "Unemployment Rate in Malaysia as of 2021",
    "width": 750,
    "height": 350,
    "projection": {"type": "equirectangular"},
    "layer": [
      {
        "data": 
        {
          "url": "/js/test.topojson",
          "format": {"type": "topojson", "feature": "states"}
        },
    
        "transform": [
          {
            "lookup": "properties.Name",
            "from": {
              "data": {
                "url": "/data/_202208230401050_labour_force_by_strata_and_state_1982_2021_dataset.csv"
              },
              "key": "State",
              "fields": ["Unemployed"]
            }
          },
          {"calculate": "datum.Unemployed *1000", "as": "Unemployed"}
        ],
        "mark": {
          "type": "geoshape",
          "stroke": "black",
          "strokeWidth":0.1
        },
        "encoding": {
          "color": {
            "field": "Unemployed",
            "type": "quantitative",
            "scale": {"type": "linear"}
          },
          "tooltip": [
            {"field": "properties.Name", "type": "nominal", "title": "State"},
            {"field": "Unemployed", "type": "quantitative","format":","}
          ]
        }
      },
      {
        "data": {
          "url": "/js/test.topojson",
          "format": {"type": "topojson", "feature": "ne_110m_graticules_1"}
        },
        "mark": {"type": "geoshape", "fill": null, "stroke": "darkgray","strokeWidth":0.2}
      }
    ]
  }